<h3>Edit post here</h3>
<!--  This is the form for to edit a post , which will be displayed at "/posts/edit"-->
<%= form_for @post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %> <br>

  <%= f.label :content %>
  <%= f.text_field :content %> <br>

  <%= f.label :user %><br>
  <%= f.collection_select :user_id, User.all, :id, :fname %>

  <br><br>

  <%= f.submit %>
<% end %>
